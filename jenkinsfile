pipeline {
  agent any
    
    tools {nodejs "node"}
    
  stages {
        
    stage('Git') {
      steps {
        git ''
      }
    }
     
    stage('Build') {
      steps {
        sh 'npm install'
      }
    }  
       stage('Test') {
      steps {
        sh 'npm start'
      }
    }
  }
}
